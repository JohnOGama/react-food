{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node10",
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "lib": ["WebWorker"]
  },
  "include": ["src"]
}
